# Cloud Connection
CLOUD_WS_URL=wss://your-app.up.railway.app/ws/agent
AGENT_ID=agent-local-01
AGENT_API_KEY=same-key-you-set-in-AGENT_API_KEYS

# WebSocket Security Configuration
# AGENT_WS_PROTOCOL=wss                    # Override protocol (wss/ws). Defaults: production=wss, development=ws
# AGENT_WS_REJECT_UNAUTHORIZED=true        # Certificate validation for WSS (default: true)

# Anthropic
ANTHROPIC_API_KEY=sk-ant-your-key

# Execution
MAX_CONCURRENT_TASKS=3
LOG_LEVEL=debug

# Resource Limits
AGENT_MAX_MEMORY_MB=2048
AGENT_MAX_CPU_PERCENT=80
AGENT_TASK_TIMEOUT_MS=1800000
AGENT_HEALTH_CHECK_INTERVAL_MS=30000

# Temporary Files
TEMP_FILE_MAX_AGE_HOURS=24
TEMP_FILE_MAX_SIZE_MB=1000
TEMP_FILE_CLEANUP_INTERVAL_MS=600000
TEMP_FILE_DIR=./temp
